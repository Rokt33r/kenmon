{
  "name": "@kenmon/react-router-adapter",
  "version": "1.0.0-pre.3",
  "description": "React Router framework adapter for Kenmon",
  "author": "Junyoung Choi <rokt33r.choi@gmail.com>",
  "license": "MIT",
  "keywords": [
    "authentication",
    "auth",
    "react-router",
    "kenmon"
  ],
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "test": "tsc --noEmit",
    "build": "tsdown",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "kenmon": "^1.0.0-pre.3",
    "react-router": "^7.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "kenmon": "workspace:*",
    "react-router": "^7.0.0",
    "typescript": "^5.0.0"
  }
}
