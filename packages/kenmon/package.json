{
  "name": "kenmon",
  "version": "1.0.0-pre.3",
  "description": "Portable authentication module - Core package",
  "author": "Junyoung Choi <rokt33r.choi@gmail.com>",
  "license": "MIT",
  "keywords": [
    "authentication",
    "auth",
    "typescript",
    "portable"
  ],
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "test": "npm run test:types && npm run test:run",
    "test:run": "vitest run",
    "test:types": "tsc --noEmit",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "build": "tsdown",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.19.24",
    "typescript": "^5.0.0",
    "vitest": "^4.0.8"
  }
}
